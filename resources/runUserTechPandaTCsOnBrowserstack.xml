<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="false" name="TechPanda">
	<parameter name="envName" value="browserstack" />

	<listeners>
		<listener class-name="commons.MethodListener" />
		<listener class-name="reportConfig.ReportNGListener" />
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="retryConfig.RetryListener" />
	</listeners>

	<test name="Run on FIREFOX browser and Windows 10">
		<parameter name="url" value="http://live.techpanda.org/index.php/" />
		<parameter name="osName" value="Windows" />
		<parameter name="osVersion" value="10" />
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.techpanda.cloud.Level_22_Manage_Data_Faker_Browserstack" />
		</classes>
	</test>
	
	<test name="Run on FIREFOX browser and Windows 11">
		<parameter name="url" value="http://live.techpanda.org/index.php/" />
		<parameter name="osName" value="Windows" />
		<parameter name="osVersion" value="11" />
		<parameter name="browser" value="edge" />
		<classes>
			<class name="com.techpanda.cloud.Level_22_Manage_Data_Faker_Browserstack" />
		</classes>
	</test>

	<test name="Run on CHROME browser and Mac OS X Monterey">
		<parameter name="url" value="http://live.techpanda.org/index.php/" />
		<parameter name="osName" value="OS X" />
		<parameter name="osVersion" value="Monterey" />
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.techpanda.cloud.Level_22_Manage_Data_Faker_Browserstack" />
		</classes>
	</test>

</suite>